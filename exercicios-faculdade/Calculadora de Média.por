programa {
  funcao inicio() {
    //Calculadora de Média: Escreva um programa que solicite ao usuário três notas, calcule a média aritmética dessas notas e a exiba na tela. 
     
     real n1, n2, n3, media

     escreva("Nota 1:")
     leia(n1)

     escreva("Nota 2:")
     leia(n2)

     escreva("Nota 3:")
     leia(n3)

     media = (n1 + n2 + n3)/3

     escreva("A média das notas é:", media)

  }
}
