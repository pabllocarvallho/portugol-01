programa {
  funcao inicio() {
    //Escreva um algoritmo em PORTUGOL que leia um número e o imprima caso ele seja maior que 20. 

   inteiro x 

   escreva("Digite um valor: ")
   leia(x)

   se(x > 20){
    escreva("valor digitado é: ", x "\n")
   }
   senao se (x == 20) {
    escreva("Valor digitado é igual a 20")
   }
   senao {
    escreva("Valor digitado é menor que 20")
   }
  }
}
